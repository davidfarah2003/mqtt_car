ew from \"@/components/FCU/Nav_overview\";\nimport Status_panel from \"@/components/Home/Status_panel\";\nimport {ref} from \"vue\";\n\nexport default {\n  name: \"Home\",\n  components: {Status_panel: Status_panel, ConnectedView, nav_overview},\n\n  setup: function () {\n    let subscribedTopics = ref([]); //Array containing the subscribed topics for this component\n    let dataObject = ref(null)\n\n    return {subscribedTopics, dataObject}\n  }\n\n\n}\n\n</script>\n\n<style scoped>\n.app-main{\n  background: var(--primary);\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr;\n  gap: 20px;\n}\n\n.h2{\n  grid-column: 2;\n  grid-row: 1;\n  border-color: #c22e2e;\n}\n\n.h1{\n  grid-column: 1;\n  grid-row: 1;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}