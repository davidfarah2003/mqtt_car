{"ast":null,"code":"import { computed, inject, onMounted, onUnmounted, ref } from \"vue\";\nexport default {\n  name: \"Home\",\n  setup: function () {\n    const subscribedTopics = ['hello', 'world', 'bitch'];\n    let test = ref(NaN);\n    let mqttClient = ref(null);\n    onMounted(() => {\n      inject('mqttClient').then(client => {\n        //mqttClient = client\n        //setupClient()\n      }).catch(err => {\n        console.log(err);\n      });\n    });\n\n    /**\n     * Execute when the component is unmounted\n     */\n    onUnmounted(() => {\n      if (mqttClient) cleanClient();\n    });\n    function setupClient() {\n      if (mqttClient) {\n        subscribedTopics.forEach(topic => {\n          mqttClient.subscribe(topic);\n          console.log(\"subscribed to \" + topic);\n        });\n        updateVariables();\n      }\n    }\n    function cleanClient() {\n      if (mqttClient) {\n        subscribedTopics.forEach(topic => {\n          mqttClient.unsubscribe(topic);\n          console.log(\"unsubscribed from \" + topic);\n        });\n      }\n    }\n    let connecteded = computed(() => {\n      return !!mqttClient;\n    });\n    function updateVariables() {\n      mqttClient.on('message', (topic, message) => {\n        console.log(\"topic: \" + topic, \",\", \"message: \" + message.toString());\n        test.value = message;\n      });\n    }\n    return {\n      test,\n      connecteded\n    };\n  }\n};","map":{"version":3,"mappings":"AASA,SAAQA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,WAAW,EAAEC,GAAG,QAAO,KAAK;AAEjE,eAAe;EACbC,IAAI,EAAE,MAAM;EAEZC,KAAK,EAAE,YAAY;IACjB,MAAMC,gBAAe,GAAI,CAAC,OAAO,EAAE,OAAO,EAAE,OAAO,CAAC;IAEpD,IAAIC,IAAG,GAAIJ,GAAG,CAACK,GAAG,CAAC;IACnB,IAAIC,UAAS,GAAIN,GAAG,CAAC,IAAI,CAAC;IAE1BF,SAAS,CAAC,MAAM;MACdD,MAAM,CAAC,YAAY,EACdU,IAAI,CAAEC,MAAM,IAAK;QAChB;QACA;MACF,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,GAAG;MACjB,CAAC,CAAC;IACR,CAAC,CAAC;;IAEF;;;IAGAX,WAAW,CAAC,MAAK;MACf,IAAGO,UAAU,EACbO,WAAW,EAAC;IACd,CAAC,CA