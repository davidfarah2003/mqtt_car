break;\n        case \"Main\":\n        default:\n          props.mqttUtil.sendCommand('main_rxpdo_1', [data]);\n          break;\n      }\n    }\n    function rxpdo2Maxon(data) {\n      switch (activeFoil.value) {\n        case \"Rear\":\n          props.mqttUtil.sendCommand('back_rxpdo_2', data);\n          break;\n        case \"Lat\":\n          props.mqttUtil.sendCommand('lat_rxpdo_2', data);\n          break;\n        case \"Main\":\n        default:\n          props.mqttUtil.sendCommand('main_rxpdo_2', data);\n          break;\n      }\n    }\n    return {\n      dataObject,\n      initialized,\n      fcuModal,\n      maxonModal,\n      battModal,\n      maxonPID,\n      configModal,\n      tuning,\n      activeFoil,\n      rxpdo1Maxon,\n      rxpdo2Maxon,\n      fanSpeed\n    };\n  }\n};","map":{"version":3,"mappings":"AA2PA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,KAAI,MAAO,wBAAwB;AAG1C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,QAAQ,EAAE;IACRF,KAAK,GAAG;MACN,OAAOA,KAAI;IACb;EACF,CAAC;EACDG,KAAK,EAAE,CAAC,WAAW,EAAE,UAAU,CAAC;EAChCC,KAAK,EAAE,CAAC,mBAAmB,EAAE,YAAY,CAAC;EAE1CC,KAAK,CAACF,KAAK,QAAU;IAAA,IAAR;MAACG;IAAI,CAAC;IACjB,IAAIC,UAAS,GAAIR,GAAG,CAAC,CAAC,CAAC;IACvB,IAAIS,WAAU,GAAIT,GAAG,CAAC,KAAK;IAE3B,MAAMU,QAAO,GAAIV,GAAG,CAAC,KAAK;IAC1B,MAAMW,UAAS,GAAIX,GAAG,CAAC,KAAK;IAC5B,MAAMY,SAAQ,GAAIZ,GAAG,CAAC,KAAK;IAC3B,MAAMa,QAAO,GAAIb,GAAG,CAAC,KAAK;IAC1B,MAAMc,WAAU,GAAId,GAAG,CAAC,KAAK;IAC7B,MAAMe,UAAS,GAAIf,GAAG,CAAC,MAAM;IAC7B,MAAMgB,QAAO,GAAIhB,GAAG,CAAC,EAAE;IAEvB,IAAIiB,cAAa,GAAI;MACnB,eAAc,EAAI,6BAA6B;MAE/C,eAAe,EAAE,+BAA+B;MAChD,eAAe,EAAE,+BAA+B;MAChD,cAAc,EAAE,+BAA+B;MAE/C,eAAe,EAAE,+BAA+B;MAChD,eAAe,EAAE,+BAA+B;MAChD,cAAc,EAAE,+BAA+B;MAE/C,cAAc,EAAE,+BAA+B;MAC/C,cAAc,EAAE,8BAA8B;MAC9C,aAAa,EAAE;IAEjB;IAEAV,IAAI,CAAC,mBAAmB,EAAEU,cAAc;IAExCC,WAAW,CAAE,MAAI;MACf,IAAId,KAAK,CAACe,SAAS,EAAE;QACnBZ,IAAI,CAAC,YAAY,EAAEU,cAAc,EAAET,UAAU;QAC7C,IAAG,CAACC,WAAW,CAACW,KAAK,EAAEX,WAAW,CAACW,KAAI,GAAI,IAAG;MAChD;IACF,GAAI,IAAI;;IAGR;;IAEA,SAASC,MAAM,CAACC,KAAK,EAAC;MAAClB,KAAK,