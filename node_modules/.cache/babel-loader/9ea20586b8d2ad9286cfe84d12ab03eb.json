{"ast":null,"code":"import { computed, inject, onMounted, onUnmounted, ref } from \"vue\";\nexport default {\n  name: \"Home\",\n  setup: function () {\n    const subscribedTopics = ['hello', 'world', 'bitch'];\n    let connected = ref(false);\n    let test = ref(NaN);\n    let mqttClient = null;\n    const clientPromise = () => {\n      inject('mqttClient').then(client => {\n        mqttClient = client;\n      }).catch(err => {\n        console.log(err);\n      });\n    };\n    onMounted(() => {\n      if (mqttClient) setupClient();\n    });\n\n    /**\n     * Execute when the component is unmounted\n     */\n    onUnmounted(() => {\n      console.log(mqttClient);\n      if (mqttClient) cleanClient();\n    });\n    function setupClient() {\n      if (mqttClient) {\n        subscribedTopics.forEach(topic => {\n          mqttClient.subscribe(topic);\n          console.log(\"subscribed to \" + topic);\n        });\n        updateVariables();\n      }\n    }\n    function cleanClient() {\n      if (mqttClient) {\n        subscribedTopics.forEach(topic => {\n          mqttClient.unsubscribe(topic);\n          console.log(\"unsubscribed from \" + topic);\n        });\n      }\n    }\n    function updateVariables() {\n      mqttClient.on('message', (topic, message) => {\n        console.log(\"topic: \" + topic, \",\", \"message: \" + message.toString());\n        test.value = message;\n      });\n    }\n    return {\n      test,\n      connected\n    };\n  }\n};","map":{"version":3,"mappings":"AASA,SAAQA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,WAAW,EAAEC,GAAG,QAAO,KAAK;AAEjE,eAAe;EACbC,IAAI,EAAE,MAAM;EAEZC,KAAK,EAAE,YAAY;IACjB,MAAMC,gBAAe,GAAI,CAAC,OAAO,EAAE,OAAO,EAAE,OAAO,CAAC;IACpD,IAAIC,SAAQ,GAAIJ,GAAG,CAAC,KAAK,CAAC;IAC1B,IAAIK,IAAG,GAAIL,GAAG,CAACM,GAAG,CAAC;IACnB,IAAIC,UAAS,GAAI,IAAI;IAErB,MAAMC,aAAY,GAAI,MAAM;MAC1BX,MAAM,CAAC,YAAY,EACdY,IAAI,CAAEC,MAAM,IAAK;QAChBH,UAAS,GAAIG,MAAK;MACpB,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,GAAG;MACjB,CAAC,CAAC;IACR;I